
@{
    ViewBag.Title = "Error";
}
<div class="page-404">
    <div class="outer">
        <div class="middle">
            <div class="inner">
                <!--BEGIN CONTENT-->
                <div class="inner-circle"><i class="fa fa-home"></i><span>404</span></div>
                <span class="inner-status">Oops! Something went wrong.</span>
                <span class="inner-detail">
                    @ViewBag.ErrorMessage;
                    <a href="#" class="btn btn-info mtl">
                        <i class="fa fa-home"></i>&nbsp;
                        Return home
                    </a>
                </span>
            </div>
        </div>
    </div>
</div>