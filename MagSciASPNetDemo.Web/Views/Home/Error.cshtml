
@{
    ViewBag.Title = "Error";
}
@section StyleSheets{
    <link href="/css/error-page.css" rel="stylesheet"/>
}
<div class="page-404">
    <div class="outer">
        <div class="middle">
            <div class="inner">
                <!--BEGIN CONTENT-->
                <div class="inner-circle"><i class="fa fa-home"></i><span>500</span></div>
                <span class="inner-status">Error: Something went wrong.</span>
                <span class="inner-detail">
                    @ViewBag.ErrorMessage
                    <a href="/" class="btn btn-info mtl">
                        <i class="fa fa-home"></i>&nbsp;
                        Return home
                    </a>
                </span>
            </div>
        </div>
    </div>
</div>